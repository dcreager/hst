# Memoization test
#
# t1 = a -> t1
# t2 = b -> t2
# u1 = t1 |~| t2
# v  = c -> P
# P  = v [] u1
# Q  = v [] u1
#
# Q should already be defined as part of the expansion of P.
# Therefore, it shouldn't create its own expansion, and should instead
# have a single Ï„ transition to the appropriate P state.

event a;
event b;
event c;

process $t1;
prefix $t1 = a -> $t1;

process $t2;
prefix $t2 = b -> $t2;

process $u1;
intchoice $u1 = $t1 |~| $t2;

process $v;
process P;

prefix $v = c -> P;

extchoice P = $v [] $u1;

process Q;
extchoice Q = $v [] $t1;
