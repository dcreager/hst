# Memoization test
#
# t1 = a -> t2
# t2 = b -> t1
# P  = t1[[ a->c, a->d ]]
# Q  = t2[[ a->c, a->d ]]
#
# Q should already be defined as part of the expansion of P.
# Therefore, it shouldn't create its own expansion, and should instead
# have a single Ï„ transition to the appropriate P state.

event a;
event b;
event c;
event d;

process $t1;
process $t2;
prefix $t1 = a -> $t2;
prefix $t2 = b -> $t1;

process P;
rename P = $t1 [[ a->c, a->d ]];

process Q;
rename Q = $t2 [[ a->c, a->d ]];
