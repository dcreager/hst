# Memoization test
#
# t1 = b -> SKIP
# t2 = a -> t1
# u1 = c -> STOP
# P  = t2 ; u1             (a -> b -> c -> STOP)
# Q  = t1 ; u1             (b -> c -> STOP)
#
# Q should already be defined as part of the expansion of P.
# Therefore, it shouldn't create its own expansion, and should instead
# have a single Ï„ transition to the appropriate P state.

event a;
event b;
event c;

process $u1;
prefix $u1 = c -> STOP;

process $t1;
prefix $t1 = b -> SKIP;
process $t2;
prefix $t2 = a -> $t1;

process P;
seqcomp P = $t2 ; $u1;

process Q;
seqcomp Q = $t1 ; $u1;
