# Memoization test
#
# t1 = a -> t2
# t2 = b -> t1
# u1 = c -> b -> u1
# P  = t1 [ {b} || {b} ] u1
# Q  = t2 [ {b} || {b} ] u1
#
# Q should already be defined as part of the expansion of P.
# Therefore, it shouldn't create its own expansion, and should instead
# have a single Ï„ transition to the appropriate P state.

event a;
event b;
event c;

process $t1;
process $t2;
prefix $t1 = a -> $t2;
prefix $t2 = b -> $t1;

process $u1;
process $u2;
prefix $u1 = c -> $u2;
prefix $u2 = b -> $u1;

process P;
aparallel P = $t1 [ {b} || {b} ] $u1;

process Q;
aparallel Q = $t2 [ {b} || {b} ] $u1;
